{
	"name": "sveltekit-demo",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"debug": "NODE_OPTIONS='--inspect' vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"studio": "prisma studio",
		"seed": "prisma db seed",
		"generate": "prisma generate",
		"prisma:push": "prisma db push",
		"prisma:pull": "prisma db pull",
		"sakura": "sass src/lib/styles/sakura/scss:src/lib/styles/sakura/css",
		"sakura:watch": "sass --watch src/lib/styles/sakura/scss:src/lib/styles/sakura/css"
	},
	"devDependencies": {
		"@csstools/normalize.css": "^12.0.0",
		"@faker-js/faker": "^8.0.2",
		"@melt-ui/pp": "^0.1.2",
		"@melt-ui/svelte": "^0.37.4",
		"@mermaid-js/mermaid-cli": "^10.3.0",
		"@playwright/test": "^1.37.0",
		"@prisma/client": "5.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.22.5",
		"@sveltejs/vite-plugin-svelte-inspector": "^1.0.3",
		"@tanstack/svelte-table": "^8.9.4",
		"@types/luxon": "^3.3.1",
		"@vincjo/datatables": "^1.12.3",
		"autoprefixer": "^10.4.14",
		"date-fns": "^2.30.0",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.30.0",
		"lucide-svelte": "^0.263.0",
		"luxon": "^3.4.0",
		"phosphor-svelte": "^1.3.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.1.1",
		"prisma-docs-generator": "^0.8.0",
		"prisma-erd-generator": "^1.11.0",
		"prisma-zod-generator": "^0.8.13",
		"sanitize.css": "^13.0.0",
		"sass": "^1.65.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-headless-table": "^0.17.5",
		"svelte-preprocess": "^5.0.4",
		"svelte-sequential-preprocessor": "^2.0.1",
		"sveltekit-flash-message": "^2.1.3",
		"sveltekit-superforms": "^1.5.1",
		"tabulator-tables": "^5.5.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.9",
		"vitest": "^0.32.2",
		"zod": "^3.21.4",
		"zod-prisma": "^0.5.4"
	},
	"type": "module",
	"dependencies": {
		"sakura.css": "^1.4.1"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	}
}